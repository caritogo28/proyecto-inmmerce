{
  "store.custom#store-location": {
    "blocks": [
      "flex-layout.row#store-location"
    ]
  },

  "flex-layout.row#store-location": {
    "children": [
      "flex-layout.col#store-location"
    ],
    "props": {
      "blockClass": "container-text-store"
    }
  },

  "flex-layout.col#store-location": {
    "props":{
      "horizontalAlign": "left"
    },
    "children": [
      "rich-text#title-tiendas",
      "flex-layout.row#tiendas-container"
    ]
  },

  "rich-text#title-tiendas": {
    "props": {
      "text":"NUESTRAS TIENDAS",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "title-store",
      "font": "t-heading-1"
    }
  },

  "flex-layout.row#tiendas-container": {
    "children": [
      "flex-layout.col#tiendas-container-left",
      "flex-layout.col#tiendas-container-right"
    ],
    "props": {
      "blockClass": "container-tiendas",
      "horizontalAlign": "between"
    }
  },

  "flex-layout.col#tiendas-container-left": {
    "props":{
      "horizontalAlign": "left",
      "width": "45%"
    },
    "children": [
      "disclosure-layout-group#TiendasLeft"
    ]
  },

  "flex-layout.col#tiendas-container-right": {
    "props":{
      "horizontalAlign": "left",
      "width": "45%"
    },
    "children": [
      "disclosure-layout-group#TiendasRight"
    ]
  },

